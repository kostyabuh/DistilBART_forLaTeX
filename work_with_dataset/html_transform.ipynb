        "\n",
        "\n",
        "import html\n",
        "\n",
        "def html_to_unicode(input_string):\n",
        "    return html.unescape(input_string)\n",
        "\n",
        "# Пример использования\n",
        "html_string = \"&#39;\"\n",
        "unicode_string = html_to_unicode(html_string)\n",
        "print(unicode_string)\n"
